{
    "name": "Linkedin Profile",
    "description": "Get Profile Linkedin",
    "version": "1.1.3222",
    "manifest_version": 3,
    "action": {
        "default_popup": "./App/popup.html",
        "default_icon": {
            "128": "./Icons/linkedin_blue_128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.linkedin.com/*"
            ],
            "js": [
                "./Scrap/utils.js",
                "./Scrap/search.js",
                "./Scrap/profile.js"
            ]
        }
    ],
    "icons": {
        "128": "./Icons/linkedin_blue_128.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}