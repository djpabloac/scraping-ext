{
    "name": "Linkedin Profile",
    "description": "Get Profile Linkedin",
    "version": "1.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "./App/index.html",
        "default_icon": {
            "128": "./Icons/linkedin_blue_128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://*.linkedin.com/*"],
            "js": ["./Scrap/utils.js"]
        }
    ],
    "icons": {
        "128": "./Icons/linkedin_blue_128.png"
    },
    "permissions": [ "activeTab", "scripting" ]
}